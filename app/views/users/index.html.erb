<div class="user-index">
  <ul>
    <%@users.each do |user|%>
      <div class="user-element">
        <li>
          <div class="add-button">
            <%= render partial: 'add_friend', locals: {user: user}%>
          </div>
          <%= link_to user.full_name, user %> 
        </li>
      </div>
    <%end%>
  </ul>
</div>